<Window x:Class="Информационно_справочная_система.Search"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Информационно_справочная_система"
        mc:Ignorable="d"
        Title="Search" Height="120" Width="190" ResizeMode="NoResize" Background="CadetBlue">
    <Grid>
        <Label x:Name="label" Content="Enter the name of the model: " HorizontalAlignment="Left" Margin="7,0,0,0" VerticalAlignment="Top"/>
        <ComboBox IsEditable="True" x:Name="comboboxSear" Background="AliceBlue" HorizontalAlignment="Left" Height="25" Margin="9,26,0,40" Width="156"/>
        <Button x:Name="button" Content="Find" Background="LightBlue" HorizontalAlignment="Left" Margin="10,63,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Cancel" Background="LightBlue" HorizontalAlignment="Left" Margin="90,63,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>

    </Grid>
</Window>
